import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int p=s.nextInt();
        if(p==0){
            System.out.println("zero");
        }
        else if(p>=-1){
            System.out.println("positive");
        }
        else{
            System.out.println("negative");
        }
    }
}
