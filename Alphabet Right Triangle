import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        char c='A';
        for(int i=1;i<=n;i++){
            
            for(int j=1;j<=i;j++){
                System.out.print(c+" ");
                c++;
            }
            c=(char)('A'+i);
            System.out.println();
        }
    }
}
