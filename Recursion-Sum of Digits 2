import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        if(n>1000)
            System.out.println("Enter a Valid Input!!!!!!!");
        else{
            while(n>0){
                sum+=n%10;
                n=n/10;
            }
            System.out.printf("Sum of Digit:%04d",sum);
        }
    }
}
