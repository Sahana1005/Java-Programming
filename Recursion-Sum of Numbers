import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        if(n<1 || n>50)
            System.out.println("Enter a Valid Input!!!!!!!!!!!!!!!!!!");
        else{
            for(int i=1;i<=n;i++)
               sum+=i; 
            System.out.printf("The sum of numbers from 1 to %d : %04d",n,sum);
        }
       
    }
}
