import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.nextLine();
        char c[]=str.toCharArray();
        for(int i=str.length()-1;i>=0;i--){
            System.out.print(c[i]);
        }
       
    }
}
