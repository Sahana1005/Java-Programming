import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0,m=n;
        while(n!=0){
            
            sum=sum+n%10;
            n=n/10;
        }
        if(m%sum==0)
            System.out.println("Harshad Number");
        else
            System.out.println("Not Harshad Number");
    }
}
